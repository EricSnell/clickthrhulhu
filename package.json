{
  "name": "clickthrhulhu",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.comp.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css",
    "compress:js": "uglifyjs js/script.js --output js/script_min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EricSnell/clickthrhulhu.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/EricSnell/clickthrhulhu/issues"
  },
  "homepage": "https://github.com/EricSnell/clickthrhulhu#readme",
  "devDependencies": {
    "autoprefixer": "^7.2.5",
    "concat": "^1.0.3",
    "eslint": "^4.15.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "postcss-cli": "^4.1.1",
    "uglify-js": "^3.3.7",
    "nodemon": "^1.19.4"
  },
  "dependencies": {
    "codemirror": "^5.49.2",
    "node-fetch": "^2.6.0",
    "papaparse": "^5.1.0"
  }
}
